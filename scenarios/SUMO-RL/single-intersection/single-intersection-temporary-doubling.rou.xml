<routes>
    <route id="route_ns" edges="n_t t_s"/>
    <route id="route_we" edges="w_t t_e"/>
    <flow id="flow_ns_1" route="route_ns" begin="0" end="1800" probability="0.25" departSpeed="max" departPos="base" departLane="best"/>    <!-- both lanes ~1800veh/hour -->
    <flow id="flow_we_1" route="route_we" begin="0" end="1800" probability="0.25" departSpeed="max" departPos="base" departLane="best"/>

    <flow id="flow_ns_2" route="route_ns" begin="1800" end="3600" probability="0.5" departSpeed="max" departPos="base" departLane="best"/>  <!-- both lanes ~3600veh/hour -->
    <flow id="flow_we_2" route="route_we" begin="1800" end="3600" probability="0.5" departSpeed="max" departPos="base" departLane="best"/>

    <flow id="flow_ns_3" route="route_ns" begin="3600" end="100000" probability="0.25" departSpeed="max" departPos="base" departLane="best"/>    <!-- both lanes ~1800veh/hour -->
    <flow id="flow_we_3" route="route_we" begin="3600" end="100000" probability="0.25" departSpeed="max" departPos="base" departLane="best"/>
</routes>
